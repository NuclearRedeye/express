{
  "name": "express-experiment",
  "version": "0.0.1",
  "description": "experiment to learn more about express.js",
  "main": "./out/index.js",
  "type": "module",
  "scripts": {
    "test": "jest",
    "predebug": "tsc",
    "debug": "node ./out/debug/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuclearredeye/express.js.git"
  },
  "keywords": [
    "typescript",
    "node.js",
    "expressjs",
    "experiment"
  ],
  "author": "NuclearRedeye",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nuclearredeye/express.js/issues"
  },
  "homepage": "https://github.com/nuclearredeye/express.js#readme",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/supertest": "^2.0.11",
    "jest": "^27.5.1",
    "jest-ts-webcompat-resolver": "^1.0.0",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "express": "^4.17.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "ts-jest"
    },
    "resolver": "jest-ts-webcompat-resolver",
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ]
  }
}
